{
  "build": {
    "beforeDevCommand": "npm run dev:fe",
    "beforeBuildCommand": "npm run build:fe",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "WaiBot",
    "version": "1.0.53"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "globalShortcut": {
        "all": true
      },
      "shell": {
        "all": true,
        "sidecar": true,
        "scope": [
          {
            "name": "bin/python/wai-bot-rpa",
            "sidecar": true,
            "args":[
              "-p",
              {
                "validator": "\\S+"
              }
            ]
          }
        ]
      },
      "fs": {
        "all": false,
        "readFile": true,
        "writeFile": true,
        "readDir": true,
        "createDir": true,
        "exists": true,
        "removeFile": true,
        "removeDir": true,
        "scope": [
          "$HOME/.wai-bot/**",
          "$DOWNLOAD/**"
        ]
      },
      "path": {
        "all": true
      },
      "os": {
        "all": true
      },
      "process": {
        "all": true,
        "exit": true,
        "relaunch": true,
        "relaunchDangerousAllowSymlinkMacos": true
      }
    },
    "systemTray": {
      "iconPath": "icons/tray-icon.png",
      "iconAsTemplate": true,
      "menuOnLeftClick": false
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": ["bin/python/wai-bot-rpa"],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "chat.wai.bot.desktop",
      "longDescription": "Wai Bot Desktop Application",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "shortDescription": "WaiBot",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": "",
        "webviewInstallMode": {
          "silent": true,
          "type": "embedBootstrapper"
        }
      }
    },
    "security": {
      "csp": null,
      "dangerousRemoteDomainIpcAccess": [
        {
          "windows": [
            "core",
            "main",
            "tray"
          ],
          "domain": "chat.openai.com",
          "enableTauriAPI": true
        },
        {
          "windows": [
            "core",
            "main"
          ],
          "domain": "labs.openai.com",
          "enableTauriAPI": true
        },
        {
          "windows": [
            "core",
            "main",
            "tray"
          ],
          "domain": "openai.com",
          "enableTauriAPI": true
        },
        {
          "windows": [
            "chatgpt_sponsors",
            "app_website"
          ],
          "domain": "app.nofwl.com",
          "enableTauriAPI": true
        }
      ]
    },
    "updater": {
      "active": true,
      "dialog": false,
      "endpoints": [
        "https://ptp-build.github.io/wai-bot-desktop/install.json"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDcxQUVGNjk2NTIwNjc3NTYKUldSV2R3WlNsdmF1Y2YvcUQ1YTlPQkdST3FoVFFrVlBMeE1vZWpCNmNQZWJLK0NuY21aWTVJamIK"
    }
  }
}
